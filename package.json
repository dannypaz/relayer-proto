{
  "name": "kinesis-relayer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "config": {
    "lnd_proto": "https://raw.githubusercontent.com/lightningnetwork/lnd/master/lnrpc/rpc.proto"
  },
  "scripts": {
    "start": "node ./src",
    "pretest": "eslint --quiet --ignore-path .gitignore .; exit 0",
    "test": "mocha",
    "dev": "nodemon ./src",
    "client": "node ./test-client.js",
    "clean": "rm -f *.log",
    "protobuf": "pbjs -t proto3 constants.proto maker.proto taker.proto orderbook.proto -p ./proto -o ./relayer.proto",
    "build": "npm run clean && npm run build-lnd-proto && npm run protobuf && npm test",
    "build-lnd-proto": "wget $npm_package_config_lnd_proto -O lnd-rpc.proto",
    "db-init": "node ./scripts/create-db.js"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "bignumber.js": "6.0.0",
    "generate-safe-id": "1.0.2",
    "grpc": "1.10.0",
    "mongojs": "2.5.0",
    "relayer-proto": "github:treygriffith/relayer-proto",
    "uuid": "3.2.1",
    "winston": "3.0.0-rc3"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "mocha": "5.0.5",
    "nodemon": "1.17.3",
    "pkg": "4.3.1",
    "protobufjs": "6.8.6"
  }
}
